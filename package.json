{
  "name": "pagelens",
  "version": "1.0.0",
  "description": "Isolated data scraping and SEO audit service for handling I/O intensive operations",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:server": "ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:manual": "node test-scripts/manual-test.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docker:build": "npm run build && docker build -t pagelens .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f pagelens",
    "docker:restart": "docker-compose restart pagelens",
    "vercel:deploy": "vercel --prod",
    "vercel:dev": "vercel dev",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bullmq": "^4.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.3.2",
    "jsdom": "^22.1.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "winston": "^3.10.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/html-to-text": "^9.0.1",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vercel/node": "^3.0.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vercel": "^32.0.0"
  },
  "keywords": [
    "data-scraping",
    "seo-audit",
    "apify",
    "serp",
    "web-scraping",
    "i/o-intensive",
    "microservice"
  ],
  "author": "",
  "license": "MIT"
}
